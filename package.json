{
  "name": "electron-app",
  "main": "dist/main.js",
  "author": "Yurii",
  "description": "electron-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "forge-start": "electron-forge start",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:electron": "ng build --prod --base-href ./ && tsc electron/main.ts electron/preload.ts --outDir dist",
    "start:electron": "electron .",
    "dev-server": "cd electron && tsc-watch --onSuccess \"electron ../dist/main.js \"",
    "dev": "concurrently \"yarn dev-server\" \"yarn start\"",
    "make": "electron-forge make",
    "package": "electron-forge package"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/cdk": "^11.0.1",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/localize": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "@ng-bootstrap/ng-bootstrap": "^8.0.0",
    "@swimlane/ngx-charts": "^16.0.0",
    "@types/electron": "^1.6.10",
    "@types/node": "^14.14.6",
    "angular-uploads": "^1.0.5",
    "bootstrap": "^4.5.0",
    "concurrently": "^5.3.0",
    "electron-compile": "^6.4.4",
    "ngx-treeview": "^10.0.2",
    "ngx-ui-loader": "^10.0.0",
    "rxjs": "~6.6.0",
    "tsc-watch": "^4.2.9",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-guru/electron-builder": "^8.0.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "electron-prebuilt-compile": "10.1.5",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "config": {
    "forge": "forge.config.js"
  }
}
